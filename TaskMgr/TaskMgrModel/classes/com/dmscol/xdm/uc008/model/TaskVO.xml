<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="TaskVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="false"
  RowClass="com.dmscol.xdm.common.framework.ViewRowImpl"
  ComponentClass="com.dmscol.xdm.common.framework.ViewObjectImpl"
  PageIterMode="Full"
  SelectList="XdmTasks.TASK_ID, 
       XdmTasks.START_DATE, 
       XdmTasks.TEXT, 
       XdmTasks.START_DATE AS START_DATE1, 
       XdmTasks.FLIGHT_NO, 
       XdmTasks.END_DATE, 
       XdmTasks.END_WHERE, 
       XdmTasks.PAX, 
       XdmTasks.ELEM_KEY, 
       XdmTasks.REMARKS"
  FromList="XDM_TASKS XdmTasks"
  OrderBy="XdmTasks.START_DATE"
  Where="(:pResponsible is null or PERS_ID = :pResponsible)
   and (:pProgramme is null or PROG_ID = :pProgramme)
   and (:pText is null or upper(TEXT) like '%' || upper(:pText) ||
   '%')">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.dmscol.xdm.uc008.model.TaskVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pResponsible"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="pProgramme"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="pText"
    Kind="where"
    Type="java.lang.String"/>
  <ViewAccessor
    Name="ServiceLOV1"
    ViewObjectName="com.dmscol.xdm.model.view.lov.ServiceLOV"
    RowLevelBinds="true"/>
  <ListBinding
    Name="ServiceLOV"
    ListVOName="ServiceLOV1"
    ListRangeSize="-1"
    NullValueFlag="start"
    NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="ElemKey"/>
    </AttrArray>
    <AttrArray Name="AttrExpressions"/>
    <AttrArray Name="ListAttrNames">
      <Item Value="ElemKey"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="Description"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <EntityUsage
    Name="XdmTasks"
    Entity="com.dmscol.xdm.model.XdmTasks"/>
  <ViewAttribute
    Name="TaskId"
    IsUpdateable="while_insert"
    PrecisionRule="true"
    EntityAttrName="TaskId"
    EntityUsage="XdmTasks"
    AliasName="TASK_ID"/>
  <ViewAttribute
    Name="StartDate"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="StartDate"
    EntityUsage="XdmTasks"
    AliasName="START_DATE">
    <Properties>
      <SchemaBasedProperties>
        <FMT_FORMATTER
          ResId="com.dmscol.xdm.uc008.model.TaskVO.StartDate_FMT_FORMATTER"/>
        <FMT_FORMAT
          ResId="com.dmscol.xdm.uc008.model.TaskVO.StartDate_FMT_FORMAT"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Text"
    PrecisionRule="true"
    EntityAttrName="Text"
    EntityUsage="XdmTasks"
    AliasName="TEXT"/>
  <ViewAttribute
    Name="StartDate1"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="StartDate"
    EntityUsage="XdmTasks"
    AliasName="START_DATE1"/>
  <ViewAttribute
    Name="FlightNo"
    PrecisionRule="true"
    EntityAttrName="FlightNo"
    EntityUsage="XdmTasks"
    AliasName="FLIGHT_NO"/>
  <ViewAttribute
    Name="EndDate"
    PrecisionRule="true"
    EntityAttrName="EndDate"
    EntityUsage="XdmTasks"
    AliasName="END_DATE"/>
  <ViewAttribute
    Name="EndWhere"
    PrecisionRule="true"
    EntityAttrName="EndWhere"
    EntityUsage="XdmTasks"
    AliasName="END_WHERE"/>
  <ViewAttribute
    Name="Pax"
    PrecisionRule="true"
    EntityAttrName="Pax"
    EntityUsage="XdmTasks"
    AliasName="PAX"/>
  <ViewAttribute
    Name="ElemKey"
    PrecisionRule="true"
    EntityAttrName="ElemKey"
    EntityUsage="XdmTasks"
    AliasName="ELEM_KEY"
    LOVName="ServiceLOV">
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="choice"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Remarks"
    PrecisionRule="true"
    EntityAttrName="Remarks"
    EntityUsage="XdmTasks"
    AliasName="REMARKS"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.dmscol.xdm.uc008.model.TaskMgrModelBundle"/>
  </ResourceBundle>
</ViewObject>
